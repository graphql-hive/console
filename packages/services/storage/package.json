    "@types/node": "18.18.13",
    "@types/pg": "8.11.0",
    "dotenv": "16.4.1",
    "fast-json-stable-stringify": "2.1.0",
    "got": "12.6.1",
    "param-case": "3.0.4",
    "pg-promise": "11.5.4",
    "slonik": "30.4.4",
    "slonik-interceptor-query-logging": "1.4.7",
    "slonik-utilities": "1.9.4",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
